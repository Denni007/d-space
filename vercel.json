{
  "routes": [
    { "handle": "filesystem" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/3d", "dest": "/new-ar.html" },

    { "src": "/contact-us", "dest": "/contact-us.html" },
    { "src": "/GDPR", "dest": "/gdpr.html" },
    { "src": "/ga4-migration", "dest": "/ga4-migration.html" },
    { "src": "/tracking", "dest": "/tracking.html" },
    { "src": "/s2s-gtm", "dest": "/s2s-gtm.html" },
    { "src": "/service/conversion-rate-optimization", "dest": "/cro-analytics.html" },

    { "src": "/sitemap.xml", "dest": "/sitmap.xml" },
    { "src": "/meet-with-us.html", "dest": "/contact-us.html" },
    { "src": "/our-team", "dest": "/our-team.html" },
    { "src": "/(.*)/js/(.*)", "dest": "/js/$2" },
    { "src": "/(.*)/css/(.*)", "dest": "/css/$2" },
    { "src": "/(.*)/images/(.*)", "dest": "/css/$2" },
    { "src": "/blogs", "dest": "/blogslist.html" },
    { "src": "/blogs/(?<slug>.*)/(.*)", "dest": "/blogs/:slug/:splat" },
    { "src": "/blog/Understanding-Meme-Coin-Development", "dest": "/blog-1.html" },
    { "src": "/blog/transforming-e-commerce-marketing-with-ibqml-a-real-life-success-story", "dest": "/blog-2.html" },

    { "src": "/(.*)", "status": 404, "dest": "/coming-soon.html" }
    
  ]
}